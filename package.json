{
  "name": "tvmanager",
  "version": "1.0.0",
  "description": "Tracks recorded, watched & upcoming TV shows",
  "keywords": [
    "tv",
    "recording",
    "iphone"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/scottohara/tvmanager.git"
  },
  "author": "Scott O'Hara",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scottohara/tvmanager/issues"
  },
  "homepage": "https://github.com/scottohara/tvmanager",
  "dependencies": {
    "@types/jquery": "3.3.5",
    "@types/jqueryui": "1.12.3",
    "@types/md5": "2.1.32",
    "@types/uuid": "3.4.3",
    "@types/websql": "0.0.27",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "0.28.11",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jquery": "3.3.1",
    "jquery-ui": "1.12.1",
    "jquery-ui-touch-punch": "0.2.3",
    "md5": "2.2.1",
    "mini-css-extract-plugin": "0.4.0",
    "ts-loader": "4.4.2",
    "typescript": "2.9.2",
    "url-loader": "1.0.1",
    "uuid": "3.2.1",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8",
    "webpack-merge": "4.1.3",
    "workbox-webpack-plugin": "3.3.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/mocha": "5.2.5",
    "@types/sinon-chai": "3.2.0",
    "chai": "4.1.2",
    "eslint": "5.2.0",
    "eslint-config-oharagroup": "3.0.0",
    "eslint-plugin-typescript": "0.12.0",
    "ignore-loader": "0.1.2",
    "istanbul-instrumenter-loader": "3.0.1",
    "jsdoc": "3.5.5",
    "karma": "2.0.3",
    "karma-chai-sinon": "0.1.5",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-ios-simulator-launcher": "0.0.4",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "4.0.0-beta.0",
    "mocha": "5.2.0",
    "open-browser-webpack-plugin": "0.0.5",
    "sinon": "6.0.0",
    "sinon-chai": "3.2.0",
    "typescript-eslint-parser": "16.0.1",
    "webpack-livereload-plugin": "2.1.1"
  },
  "scripts": {
    "start": "heroku local --procfile Procfile.dev",
    "postinstall": "npm run build",
    "build": "webpack --progress --config webpack.prod.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "lint": "eslint 'src/**/*.{ts,js}' 'spec/public/**/*.{ts,js}' '*.{ts,js}'",
    "test": "npm run lint && npm run test:coverage:mobile",
    "test:bdd": "karma start karma-bdd.conf.js",
    "test:coverage": "karma start",
    "test:coverage:chrome": "karma start --browsers ChromeHeadless",
    "test:coverage:mobile": "karma start --browsers MobileSafari",
    "docs": "jsdoc --configure jsdoc.json"
  },
  "engines": {
    "node": "10.12.0",
    "npm": "6.4.1"
  }
}
