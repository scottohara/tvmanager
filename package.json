{
  "name": "tvmanager",
  "version": "1.0.0",
  "description": "iPhone web app for managing recorded TV",
  "keywords": [
    "tv",
    "recording",
    "iphone"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/scottohara/tvmanager.git"
  },
  "author": "Scott O'Hara",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scottohara/tvmanager/issues"
  },
  "homepage": "https://github.com/scottohara/tvmanager",
  "dependencies": {
    "babel-minify-webpack-plugin": "0.2.0",
    "clean-webpack-plugin": "0.1.17",
    "copy-webpack-plugin": "4.2.0",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "1.1.5",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "jquery": "3.1.1",
    "jquery-ui": "1.12.1",
    "jquery-ui-touch-punch": "0.2.3",
    "md5": "2.2.1",
    "url-loader": "0.6.2",
    "uuid": "3.0.1",
    "webpack": "3.10.0",
    "webpack-merge": "4.1.1"
  },
  "devDependencies": {
    "chai": "4.1.1",
		"eslint": "4.12.0",
    "eslint-config-oharagroup": "2.3.0",
    "ignore-loader": "0.1.2",
    "istanbul-instrumenter-loader": "3.0.0",
    "jsdoc": "3.4.3",
    "karma": "1.7.0",
    "karma-chai-sinon": "0.1.5",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-ios-simulator-launcher": "0.0.4",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.3",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.5",
    "mocha": "3.5.0",
    "open-browser-webpack-plugin": "0.0.5",
    "sinon": "2.4.1",
    "sinon-chai": "2.12.0",
    "webpack-livereload-plugin": "1.0.0"
  },
  "scripts": {
    "start": "heroku local --procfile Procfile.dev",
    "postinstall": "npm run build",
    "build": "webpack --progress --config webpack.prod.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "lint": "eslint 'src/**/*.js' 'spec/public/**/*.js' '*.js'",
    "test": "npm run lint && npm run test:coverage",
    "test:bdd": "karma start karma-bdd.conf.js",
    "test:coverage": "karma start",
    "docs": "jsdoc --configure jsdoc.json"
  },
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  }
}
